{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "prestorybook": "npm run test",
    "storybook": "node ./server/server.js | start-storybook -p 9010",
    "build": "npm run test-ci && cross-env NODE_ENV=production webpack -p --config webpack.dist.config.js",
    "publish-module": "publish",
    "postversion": "git push origin && git push origin --tags",
  },
  "devDependencies": {
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "cheerio": "^0.20.0",
    "cross-env": "^3.1.2",
    "css-loader": "~0.16.0",
    "enzyme": "^2.2.0",
    "es5-shim": "~4.1.10",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "https-proxy-agent": "*",
    "jsdom": "^8.3.1",
    "json-server": "^0.8.22",
    "kadira-storybook": "^1.19.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "markdown-to-react-components": "^0.2.1",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.12.0",
    "publish": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react": "^15.0.0",
    "react-addons-create-fragment": "^15.3.1",
    "redux": "^3.5.2",
    "sinon": "^1.17.3",
    "style-loader": "~0.12.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "babel-runtime": "^6.5.0",
    "loglevel": "^1.4.1",
    "radium": "^0.18.1",
    "react-addons-test-utils": "^15.0.0",
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
